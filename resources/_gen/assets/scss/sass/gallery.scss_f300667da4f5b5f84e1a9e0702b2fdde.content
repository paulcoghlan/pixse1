.photo-container {
  display: flex;
  flex-wrap: wrap;
  margin: 0 -40px; }
  @media (max-width: 899px) {
    .photo-container {
      margin: 0; } }
  .photo-container img {
    border-radius: 8px; }
  .photo-container .photo-item {
    padding: 2px;
    width: 50%; }
    @media (max-width: 683px) {
      .photo-container .photo-item {
        width: 100%; } }
